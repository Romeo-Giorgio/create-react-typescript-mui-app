    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run tsc && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write --parser typescript",
