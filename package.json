{
  "name": "@romgio/create-react-typescript-mui-app",
  "version": "1.0.9",
  "bin": "./bin/cli.js",
  "main": "src/index.ts",
  "author": "RomÃ©o Giorgio",
  "description": "Starter for a React Typesrcipt app using material ui",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
